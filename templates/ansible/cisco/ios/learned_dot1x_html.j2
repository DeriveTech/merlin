<html><head><link rel="stylesheet" href="../../../../styles.css"></head><body>
<h1>Learn Dot1x/</h1>
<table class="merlin-table">
  <thead>
  <tr>
    <th>Interface</th>
    <th>Client</th>
    <th>EAP Method</th>
    <th>PAE</th>
    <th>Status</th>
    <th>Session</th>
    <th>Auth Bend</th>
    <th>Auth SM State</th>
    <th>Max Reauth</th>
    <th>Max Requests</th>
    <th>Last RX MAC</th>
    <th>RX Invalid</th>
    <th>RX Error</th>
    <th>RX Logoff</th>
    <th>RX Response</th>
    <th>RX Start</th>
    <th>RX Total</th>
    <th>TX Requests</th>
    <th>TX Request ID</th>
    <th>TX Total</th>
    <th>Quiet Period</th>
    <th>Server Timeout</th>
    <th>Supplicant Timeout</th>
    <th>TX Period</th>  
  </tr>
  </thead>
  <tbody>
  {% for interface in learned_dot1x.genie.dot1x.interfaces %}
  {% if learned_dot1x.genie.dot1x.interfaces[interface].clients is defined %}
  {% for client in learned_dot1x.genie.dot1x.interfaces[interface].clients %}
  {% for session in learned_dot1x.genie.dot1x.interfaces[interface].clients[client].session %}
  <tr>
    <td>{{ interface }}</td>
    <td>{{ client }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].clients[client].eap_method }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].clients[client].pae }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].clients[client].status }}</td>
    <td>{{ session }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].clients[client].session[session].auth_bend_sm_state }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].clients[client].session[session].auth_sm_state }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].max_reauth_req }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].max_req }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.lastrxsrcmac }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxinvalid }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxlenerr }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxlogoff }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxresp }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxstart }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxtotal }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.txreq }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.txreqid }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.txtotal }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].timeout.quiet_period }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].timeout.server_timeout }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].timeout.supp_timeout }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].timeout.tx_period }}</td>
    {% endfor %}
    {% endfor %}
    {% else %}
    <td>{{ interface }}</td>
    <td>No Client</td>
    <td>N/A</td>
    <td>N/A</td>
    <td>N/A</td>
    <td>No Sessions</td>
    <td>N/A</td>
    <td>N/A</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].max_reauth_req }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].max_req }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.lastrxsrcmac }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxinvalid }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxlenerr }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxlogoff }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxresp }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxstart }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.rxtotal }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.txreq }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.txreqid }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].statistics.txtotal }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].timeout.quiet_period }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].timeout.server_timeout }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].timeout.supp_timeout }}</td>
    <td>{{ learned_dot1x.genie.dot1x.interfaces[interface].timeout.tx_period }}</td>    
  </tr>
  {% endif %}
  {% endfor %}
  </tbody>  
</table>
</body></html>